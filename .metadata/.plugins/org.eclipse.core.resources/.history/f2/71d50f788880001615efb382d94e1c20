package com.rokid.rkcommonlibrary.listener;

import com.rokid.rkcommonlibrary.beans.RKEventBean;

/**
 * Created by Bassam on 16/8/17.
 */
public interface RKOnRightTouchListener {
    /**
     * 按下事件,子线程回调
     *
     * @param rkEventBean
     */
    void onRightTouchDown(RKEventBean rkEventBean);

    /**
     * 变化
     *
     * @param rkEventBean
     */
    void onRightTouchMove(RKEventBean rkEventBean);

    /**
     * 抬起
     */
    void onRightTouchUp();
}

