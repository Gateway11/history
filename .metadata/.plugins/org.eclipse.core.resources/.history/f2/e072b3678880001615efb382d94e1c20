package com.rokid.simple.datetime;

import com.google.gson.Gson;

/**
 * Created by Bassam on 16/7/4.
 */
public class DateTimeUtil {
    public DateTimeBean getDateTimeBean(String dateTime) {
        try {
            Gson gson = new Gson();
            return gson.fromJson(dateTime, DateTimeBean.class);
        } catch (Exception ex) {
            ex.printStackTrace();
        }
        return null;
    }
}
