package com.rokid.rkcommonlibrary.beans;

import java.io.Serializable;

/**
 * Created by Bassam on 16/7/21.
 */
public class RKEventBean implements Serializable {
    private String device;
    private RKEventValueBean event;

    public String getDevice() {
        return device;
    }

    public void setDevice(String device) {
        this.device = device;
    }

    public RKEventValueBean getEvent() {
        return event;
    }

    public void setEvent(RKEventValueBean event) {
        this.event = event;
    }
}
