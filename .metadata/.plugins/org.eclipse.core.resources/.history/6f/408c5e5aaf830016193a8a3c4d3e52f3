package com.rokid.base;

import rokid.home.Vendor;

import android.os.Parcel;
import android.os.Parcelable;

public class VirtualScene implements Parcelable {
	private long id;
	private String sceneId;
	private String name;
	private Vendor vendor;
	private String trigger;
	
	public VirtualScene() {
		super();
	}

	public VirtualScene(Parcel source) {
		this.id = source.readLong();
		this.sceneId = source.readString();
		this.name = source.readString();
		this.vendor = source.readParcelable(Vendor.class.getClassLoader()); 
		this.trigger = source.readString();
	}

	@Override
	public int describeContents() {
		return 0;
	}

	@Override
	public void writeToParcel(Parcel dest, int flags) {
		dest.writeLong(id);
		dest.writeString(sceneId);
		dest.writeString(name);
		dest.writeParcelable(vendor, 0);
		dest.writeString(trigger);
	}
	
	public static final Creator<VirtualScene> CREATOR = new Creator<VirtualScene>() {
		@Override
		public VirtualScene createFromParcel(Parcel source) {
			return new VirtualScene(source);
		}

		@Override
		public VirtualScene[] newArray(int size) {
			return new VirtualScene[size];
		}
	};

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getSceneId() {
		return sceneId;
	}

	public void setSceneId(String sceneId) {
		this.sceneId = sceneId;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Vendor getVendor() {
		return vendor;
	}

	public void setVendor(Vendor vendor) {
		this.vendor = vendor;
	}

	public String getTrigger() {
		return trigger;
	}

	public void setTrigger(String trigger) {
		this.trigger = trigger;
	}

	
}
